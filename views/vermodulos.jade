extends layout
extends menu
block content
  title Módulos
  //Contenido de Pagina
  div#page-wrapper
    div.container-fluid
      div.row
        div.col-lg-12
          p #{mensaje}
          header #{titulo}
      div.col-lg-12
        table.table.table.striped
          thead
            th id del módulo
            th Administrador
            th Nombre
            th Número
          tbody
            each modulo in modulos
              tr
                td #{modulo.idModulo}
                td #{modulo.usuarioAdministrador}
                td #{modulo.nombre}
                td #{modulo.numeroModulo}
        a(href='') Nueva Huerta
  //Cierro segundo col-lg-12 
  //Cierro Container Fluid
  //Containererro PageWrapper
  //Cierro Wrapper
  //Jquery
  script(src='/bootstrap/js/jquery.js')
  //Bootstrap Core JS
  script(src='/bootstrap/js/bootstrap.min.js')
